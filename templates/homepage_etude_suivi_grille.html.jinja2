{% extends "homepage_etude_suivi.html.jinja2" %}

{% block gridMission %}
    <form>
        {% for mission in missions %}
            <div class="input-group">
                <div class="input-group-prepend">
                    <div class="input-group-text">
                        <input type="checkbox">
                    </div>
                </div>
                <input type="text" class="form-control" placeholder="{{ mission.name }}">
            </div>
        {% endfor %}
    </form>
{% endblock %}
